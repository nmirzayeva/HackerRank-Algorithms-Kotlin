fun compareTriplets(a: Array<Int>, b: Array<Int>): Array<Int> {
    var sum = arrayOf<Int>(0, 0)


    for (i in 0..a.size - 1) {


        if (a[i] > b[i]) {
            sum[0] += 1

        } else if (a[i] == b[i]) {
            sum[0] += 0
            sum[1] += 0
        } else {

            sum[1] += 1
        }


    }

    return sum

}
